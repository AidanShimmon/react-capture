  }\n`;\n\n// Award Component\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/aidanshimmon/Documents/Web projects/Learning projects/React/Dev Ed/Capture/capture/src/movieState.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":9,"column":18,"nodeType":"69","messageId":"70","endLine":9,"endColumn":27},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]