tory();\n  const url = history.location.pathname;\n  const [movies, setMovies] = useState(MovieState);\n  const [movie, setMovie] = useState(null);\n\n  // Use Effect\n  useEffect(() => {\n    const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\n    setMovie(currentMovie[0]);\n  }, [movies, url]);\n\n  return (\n    <>\n      {movie && (\n        <Details\n          variants={pageAnimation}\n          initial=\"hidden\"\n          animate=\"show\"\n          exit=\"exit\"\n        >\n          <HeadLine>\n            <h2>{movie.title}</h2>\n            <img src={movie.mainImg} alt=\"movie\" />\n          </HeadLine>\n          <Awards>\n            {movie.awards.map((award) => (\n              <Award\n                title={award.title}\n                description={award.description}\n                key={award.title}\n              />\n            ))}\n          </Awards>\n          <ImageDisplay>\n            <img src={movie.secondaryImg} alt=\"secondary\" />\n          </ImageDisplay>\n        </Details>\n      )}\n    </>\n  );\n};\n\nconst Details = styled(motion.div)`\n  color: white;\n`;\n\nconst HeadLine = styled.div`\n  min-height: 90vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\n\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n  @media (max-width: 1300px) {\n    display: block;\n    margin: 2rem;\n  }\n`;\n\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0 1rem;\n  }\n  p {\n    padding: 2rem 0rem;\n  }\n`;\n\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100%;\n    obje